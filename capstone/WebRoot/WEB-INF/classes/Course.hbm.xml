<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.javabean.Course" table="Course">
<id name="cid" column="cid" type="String">
</id>
<property name="title" column="title" type="string"></property>
<property name="credits" column="credits" type="int"></property>
<property name="offering" column="offering" type="string"></property>
<property name="prerequisites" column="prerequisites" type="string"></property>
<property name="lab" column="lab" type="string"></property>

</class>
</hibernate-mapping>